<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property>
        <property name="hibernate.connection.driver_class">org.postgresql.Driver</property>
        <property name="hibernate.connection.url">jdbc:postgresql://localhost:5432/</property>
        <property name="hibernate.connection.username">postgres</property>
        <property name="hibernate.connection.password">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.PostgreSQLDialect</property>
        <property name="show_sql">true</property>
        <mapping class="com.tp.dominio.geo.Pais"></mapping>
        <mapping class="com.tp.dominio.pasajero.TipoDocumento"></mapping>
        <mapping class="com.tp.dominio.habitacion.TipoHabitacion"></mapping>
        <mapping class="com.tp.dominio.habitacion.CostoPorNoche"></mapping>
        <mapping class="com.tp.dominio.factura.items.Servicio"></mapping>
        <mapping class="com.tp.dominio.factura.items.ItemFactura"></mapping>
        <mapping class="com.tp.dominio.geo.Provincia"></mapping>
        <mapping class="com.tp.dominio.geo.Ciudad"></mapping>
        <mapping class="com.tp.dominio.direccion.Direccion"></mapping>
        <mapping class="com.tp.dominio.factura.Factura"></mapping>
        <mapping class="com.tp.dominio.factura.cancelacion.Pago"></mapping>
        <mapping class="com.tp.dominio.habitacion.Habitacion"></mapping>
        <mapping class="com.tp.dominio.reserva.Reserva"></mapping>
        <mapping class="com.tp.dominio.reserva.ResponsableReserva"></mapping>
        <mapping class="com.tp.dominio.factura.cancelacion.NotaDeCredito"></mapping>
        <mapping class="com.tp.dominio.factura.ResponsablePagoTercero"></mapping>
        <mapping class="com.tp.dominio.ocupacion.Ocupacion"></mapping>
        <mapping class="com.tp.dominio.pasajero.Pasajero"></mapping>
        <mapping class="com.tp.dominio.pasajero.PosicionIVA"></mapping>
        <mapping class="com.tp.dominio.medioDePago.tarjeta.TipoTarjeta"></mapping>
        <mapping class="com.tp.dominio.medioDePago.Banco"></mapping>
        <mapping class="com.tp.dominio.medioDePago.Moneda"></mapping>
        <mapping class="com.tp.dominio.medioDePago.MedioDePago"></mapping>
        <mapping class="com.tp.dominio.medioDePago.otro.Cheque"></mapping>
        <mapping class="com.tp.dominio.medioDePago.otro.Efectivo"></mapping>
        <mapping class="com.tp.dominio.medioDePago.tarjeta.TarjetaDeCredito"></mapping>
        <mapping class="com.tp.dominio.medioDePago.tarjeta.TarjetaDeDebito"></mapping>
    </session-factory>
</hibernate-configuration>